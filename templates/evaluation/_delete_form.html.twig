<form method="post" action="{{ path('evaluation_delete', {'id': evaluation.id}) }}" onsubmit="return confirm('Êtes-vous sûre de vouloir supprimer cet objet ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ evaluation.id) }}">
    <button class="btn btn-danger float-right">Supprimer</button>
</form>
